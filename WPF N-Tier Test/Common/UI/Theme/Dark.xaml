<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:system="clr-namespace:System;assembly=System.Runtime">
  <Color x:Key="ShadowC">#424242</Color>


  <SolidColorBrush x:Key="MainSpace" Color="#161616" />
  <SolidColorBrush x:Key="MainSpaceLighter1" Color="#1B1B1B" />
  <SolidColorBrush x:Key="MainSpaceLighter2" Color="#202020" />
  <SolidColorBrush x:Key="MainSpaceDarker1" Color="#141414" />
  <SolidColorBrush x:Key="MainSpaceDarker2" Color="#101010" />

  <SolidColorBrush x:Key="MainContrast" Color="#c8c8c8" />

  <SolidColorBrush x:Key="ButtonC" Color="#0a0a0a" />
  <SolidColorBrush x:Key="ButtonCHover" Color="#0e0e0e" />

  <SolidColorBrush x:Key="ActionButtonC" Color="#0c0e11" />
  <SolidColorBrush x:Key="ActionButtonCHover" Color="#161616" />

  <SolidColorBrush x:Key="MainFont" Color="#dddddd" />
  <SolidColorBrush x:Key="SecondFont" Color="#ffffff" />
  <SolidColorBrush x:Key="ContrastFont" Color="#000000" />
  <SolidColorBrush x:Key="ContrastFont2" Color="#0c0e11" />
  <SolidColorBrush x:Key="CellSelectionFont" Color="#000000" />

  <SolidColorBrush x:Key="MainBorder" Color="#000000" />
  <SolidColorBrush x:Key="SecondBorder" Color="#0c0e11" />

  <SolidColorBrush x:Key="Separator" Color="#202020" />
  <SolidColorBrush x:Key="Shadow" Color="#424242" />

</ResourceDictionary>