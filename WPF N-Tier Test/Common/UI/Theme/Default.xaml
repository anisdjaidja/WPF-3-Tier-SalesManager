<ResourceDictionary
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
  xmlns:system="clr-namespace:System;assembly=System.Runtime">
  <Color x:Key="ShadowC">#FFEDEDED</Color>

  <SolidColorBrush x:Key="MainSpace" Color="#f3f3f3" />
  <SolidColorBrush x:Key="MainSpaceLighter1" Color="#fafafa" />
  <SolidColorBrush x:Key="MainSpaceLighter2" Color="#ffffff" />
  <SolidColorBrush x:Key="MainSpaceDarker" Color="#f0f0f0" />
  <SolidColorBrush x:Key="MainSpaceDarker1" Color="#ebebeb" />
  <SolidColorBrush x:Key="MainSpaceDarker2" Color="#e6e6e6" />

  <SolidColorBrush x:Key="MainContrast" Color="#2e2e2e" />

  <SolidColorBrush x:Key="ButtonC" Color="#ffffff" />
  <SolidColorBrush x:Key="ButtonCHover" Color="#f6f6f6" />

  <SolidColorBrush x:Key="ActionButtonC" Color="Transparent" />
  <SolidColorBrush x:Key="ActionButtonCHover" Color="#f8f8f8" />

  <SolidColorBrush x:Key="MainFont" Color="#000000" />
  <SolidColorBrush x:Key="SecondFont" Color="#0c0e11" />
  <SolidColorBrush x:Key="ContrastFont" Color="#dddddd" />
  <SolidColorBrush x:Key="ContrastFont2" Color="#ffffff" />
  <SolidColorBrush x:Key="CellSelectionFont" Color="#000000" />

  <SolidColorBrush x:Key="MainBorder" Color="Gray" />
  <SolidColorBrush x:Key="SecondBorder" Color="DimGray" />

  <SolidColorBrush x:Key="Separator" Color="LightGray" />
  <SolidColorBrush x:Key="Shadow" Color="#FFEDEDED" />

</ResourceDictionary>